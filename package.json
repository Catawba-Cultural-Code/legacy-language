{
  "name": "language-project",
  "version": "0.0.1",
  "description": "Framework for language apps",
  "main": "server/index.js",
  "proxy": "http://localhost:3333/",
  "scripts": {
    "dev": "concurrently \"nodemon server/index.js\" \"npm run start:react\"",
    "start": "node server/index.js",
    "client-build": "cd app && npm run build",
    "commit": "git add . && git commit -am",
    "deploy": "git push heroku master && git push origin master",
    "start:react": "react-scripts start",
    "build": "react-scripts build"
  },
  "keywords": [
    "js",
    "node",
    "react",
    "express",
    "language",
    "app",
    "learning"
  ],
  "author": "delesslin",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
